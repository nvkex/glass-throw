{
    "name" : "GlassThrow",
    "description" : "Block annyoing signup popups on glassdoor.",
    "version" : "1.0.0",
    "manifest_version":2,
    "icons":{},
    "background":{
        "scripts":["./background.js"]
    },
    "browser_action" : {
        "default_popup" : "popup.html"
    },
    "options_page":"./options.html",
    "permissions":[
        "tabs",
        "https://www.glassdoor.com/*",
        "https://www.glassdoor.co.in/*"
    ]
}